<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.scrumit.entity.Contact"
          extends="_local"
          name="contact-view">
        <property name="specialities"
                  view="_minimal"/>
        <property name="city"
                  view="_minimal"/>
        <property name="status"
                  view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_local"
          name="tasks-full">
        <property name="parentBug" view="_local"/>
        <property name="estimation" view="_minimal"/>
        <property name="performer" view="_local"/>
        <property name="task" view="_minimal">
            <property name="task" view="_minimal"/>
            <property name="level"/>
            <property name="top" view="_minimal"/>
        </property>
        <property name="top" view="_minimal">
            <property name="task" view="_minimal"/>
        </property>
        <property name="sprintBacklog" view="_minimal"/>
        <property name="teams" view="_minimal"/>
        <property name="sprints" view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_minimal"
          name="task-tree">
        <property name="task"
                  view="_minimal"/>
        <property name="level"/>
        <property name="top"
                  view="_minimal"/>
        <property name="description"/>
        <property name="done"/>
        <property name="realduration"/>
        <property name="type"/>
        <property name="deadline"/>
        <property name="begin"/>
        <property name="duration"/>
        <property name="amount"/>
        <property name="control"/>
        <property name="priority"/>
        <property name="performer"
                  view="_minimal"/>
        <property name="testingPlan"/>
        <property name="planningTime"/>
        <property name="actualTime"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_minimal"
          name="tasks-performer-view">
        <property name="task"
                  view="_minimal"/>
        <property name="begin"/>
        <property name="deadline"/>
        <property name="done"/>
        <property name="control"/>
        <property name="priority"/>
        <property name="planningTime"/>
        <property name="actualTime"/>
        <property name="performer"
                  view="_local"/>
        <property name="testingPlan"/>
    </view>
    <view class="com.company.scrumit.entity.TaskComment"
          extends="_local"
          name="taskComment-browse">
        <property name="createTs"/>
        <property name="createdBy"/>
    </view>
    <view class="com.company.scrumit.entity.Team"
          extends="_local"
          name="team-view">
        <property name="leader"
                  view="_minimal"/>
        <property name="projects"
                  view="_minimal"/>
        <property name="members"
                  view="_minimal">
            <property name="email"/>
        </property>
    </view>
    <view class="com.company.scrumit.entity.Sprint"
          extends="_local"
          name="sprint-view">
        <property name="team"
                  view="_minimal">
            <property name="smartsheetId"/>
        </property>
        <property name="tasks"
                  view="_minimal">
            <property name="deadline"/>
            <property name="begin"/>
            <property name="done"/>
            <property name="realduration"/>
            <property name="type"/>
            <property name="description"/>
            <property name="performer"
                      view="_minimal"/>
            <property name="duration"/>
            <property name="amount"/>
            <property name="task"
                      view="_minimal">
                <property name="ssId"/>
            </property>
            <property name="control"/>
            <property name="ssId"/>
            <property name="priority"/>
        </property>
    </view>
    <view class="com.company.scrumit.entity.Performer"
          extends="_minimal"
          name="performer-view">
        <property name="contact"
                  view="_minimal"/>
        <property name="teams"
                  view="_minimal"/>
        <property name="loginLowerCase"/>
        <property name="password"/>
        <property name="firstName"/>
        <property name="lastName"/>
        <property name="middleName"/>
        <property name="email"/>
        <property name="changePasswordAtNextLogon"/>
        <property name="group"
                  view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_minimal"
          name="task-top">
        <property name="top"
                  view="_minimal"/>
        <property name="level"/>
        <property name="performer"
                  view="_minimal"/>
        <property name="testingPlan"/>
    </view>
    <view class="com.company.scrumit.entity.Meeting"
          extends="_local"
          name="meeting-view">
        <property name="sprint"
                  view="_minimal">
            <property name="tasks"
                      view="_minimal">
                <property name="task"
                          view="_minimal"/>
            </property>
        </property>
    </view>
    <view class="com.company.scrumit.entity.MeetingsTask"
          extends="_local"
          name="meetingsTask-full">
        <property name="meeting"
                  view="_minimal"/>
        <property name="task"
                  view="_minimal">
            <property name="done"/>
            <property name="control"/>
            <property name="task"
                      view="_minimal"/>
            <property name="performer"
                      view="_minimal"/>
            <property name="deadline"/>
            <property name="begin"/>
        </property>
    </view>
    <view class="com.company.scrumit.entity.Tracker"
          extends="_local"
          name="_full">
        <property name="project"
                  view="tasks-full"/>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="performer"
                  view="_minimal"/>
        <property name="workflow"
                  view="_minimal"/>
        <property name="task"
                  view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Tracker"
          extends="_minimal"
          name="tracker-process">
        <property name="workflow"
                  view="_minimal"/>
        <property name="stepName"/>
        <property name="status"/>
        <property name="performer"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.workflowstp.entity.Workflow"
          extends="_minimal"
          name="tracker-workflow-browse">
        <property name="steps"
                  view="_minimal">
            <property name="stage"
                      view="_minimal">
                <property name="actors"
                          view="_minimal"/>
                <property name="actorsRoles"
                          view="_minimal"/>
                <property name="viewers"
                          view="_minimal"/>
                <property name="viewersRoles"
                          view="_minimal"/>
                <property name="browseScreenGroovyScript"/>
            </property>
        </property>
    </view>
    <view class="com.haulmont.cuba.security.entity.User"
          extends="_local"
          name="user-with-role">
        <property name="userRoles"
                  view="_minimal">
            <property name="user"
                      view="_minimal"/>
            <property name="role"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.company.scrumit.entity.Performer"
          extends="_local"
          name="user-with-roles">
        <property name="userRoles"
                  view="_minimal">
            <property name="user"
                      view="_minimal"/>
            <property name="role"
                      view="_minimal"/>
        </property>
        <property name="teams"
                  view="_minimal">
            <property name="projects"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.company.scrumit.entity.Files"
          extends="_local"
          name="files-browser">
        <property name="file"
                  view="_minimal">
            <property name="size"/>
        </property>
    </view>
</views>
